    {
      "parameters": {
        "jsCode": "const events = $input.all();\nconst userData = $input.first().json.userData;\nconst senderId = $input.first().json.senderId;\nconst date = $input.first().json.date;\n\n// Define time slots\nconst businessSlots = [\n  { start: '09:00', end: '10:00', id: 'slot_1', display: '09:00 - 10:00' },\n  { start: '10:00', end: '11:00', id: 'slot_2', display: '10:00 - 11:00' },\n  { start: '11:00', end: '12:00', id: 'slot_3', display: '11:00 - 12:00' },\n  { start: '13:00', end: '14:00', id: 'slot_4', display: '13:00 - 14:00' },\n  { start: '14:00', end: '15:00', id: 'slot_5', display: '14:00 - 15:00' },\n  { start: '15:00', end: '16:00', id: 'slot_6', display: '15:00 - 16:00' },\n  { start: '16:00', end: '17:00', id: 'slot_7', display: '16:00 - 17:00' },\n  { start: '17:00', end: '18:00', id: 'slot_8', display: '17:00 - 18:00' }\n];\n\n// Get booked times\nconst bookedTimes = events.map(event => {\n  if (!event.json.start) return null;\n  const startTime = event.json.start.dateTime || event.json.start.date;\n  const eventDate = new Date(startTime);\n  const hours = String(eventDate.getHours()).padStart(2, '0');\n  const minutes = String(eventDate.getMinutes()).padStart(2, '0');\n  return `${hours}:${minutes}`;\n}).filter(time => time !== null);\n\n// Filter available slots\nconst availableSlots = businessSlots.filter(slot => {\n  return !bookedTimes.includes(slot.start);\n});\n\nif (availableSlots.length === 0) {\n  return {\n    senderId: senderId,\n    message: \"üòî Sorry, this day is fully booked. Please choose another date:\",\n    stage: 'awaiting_date',\n    userData: userData,\n    dayFull: true\n  };\n}\n\n// Create quick replies\nconst quickReplies = availableSlots.slice(0, 11).map(slot => ({\n  content_type: \"text\",\n  title: slot.display,\n  payload: slot.id\n}));\n\nuserData.availableSlots = availableSlots;\n\nreturn {\n  senderId: senderId,\n  userData: userData,\n  availableSlots: availableSlots,\n  quickReplies: quickReplies,\n  message: \"Available time slots for \" + date + \": ‚è∞\",\n  stage: 'showing_slots',\n  dayFull: false\n};"
      },
